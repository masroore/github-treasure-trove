{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "buzz/laravel-google-captcha": "^2.1",
        "davejamesmiller/laravel-breadcrumbs": "^5.2",
        "doctrine/dbal": "~2.3",
        "fideloper/proxy": "^4.0",
        "fomvasss/bitrix24-api-hook": "^0.1.1",
        "fomvasss/laravel-eus": "^0.2.1",
        "fomvasss/laravel-meta-tags": "^2.0",
        "fomvasss/laravel-taxonomy": "^3.0",
        "fomvasss/laravel-url-aliases": "^3.10",
        "fomvasss/laravel-variables": "^3.7",
        "itspace/externalshop": "dev-develop",
        "kyslik/column-sortable": "^5.7",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.7",
        "laravelium/sitemap": "3.0.*",
        "nwidart/laravel-modules": "^6.2",
        "orangehill/iseed": "^3.0",
        "rap2hpoutre/laravel-log-viewer": "^1.4",
        "sanmai/cdek-sdk": "^0.6.16",
        "spatie/laravel-honeypot": "^1.3",
        "spatie/laravel-medialibrary": "^7.5",
        "spatie/laravel-permission": "^2.30",
        "spatie/laravel-web-tinker": "1.2.2",
        "unisharp/laravel-filemanager": "v1.8",
        "waavi/sanitizer": "dev-master",
        "zlodey/laravel-sendpulse-api": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "LaravelTrailingSlash\\RoutingServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Overrides\\": "overrides/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "Modules/*"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
