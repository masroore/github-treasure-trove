{
    "name": "fr0tt/benotes",
    "description": "App for bookmarks and notes.",
    "keywords": ["laravel", "lumen", "bookmark", "notes"],
    "license": "MIT",
    "version": "2.0.6",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "doctrine/dbal": "^2.12",
        "fideloper/proxy": "^4.2",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/mail": "^7.0",
        "illuminate/notifications": "^7.0",
        "intervention/image": "^2.5",
        "ksubileau/color-thief-php": "^1.4",
        "laravel/lumen-framework": "^7.0",
        "laravel/tinker": "^2.6",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "ramsey/uuid": "^4.1",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
